{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/embassies",
        "dest": "/api/embassies.js"
      },
      {
        "src": "/embassies/search",
        "dest": "/api/search.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/",
        "dest": "/public/index.html"
      }
    ]
  }